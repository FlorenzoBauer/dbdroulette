{
  "name": "trialrandomizer",
  "version": "1.1.2",
  "private": false,
  "main": "electron/electron.js",
  "homepage": "./",
  "description": "Trial Randomizer - Random loadouts",
  "author": {
    "name": "florenzobauer",
    "email": "myemail@email.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FlorenzoBauer/trialrandomizer"
  },
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "electron-is-dev": "^3.0.1",
    "electron-updater": "^6.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
   "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron-forge start",
    "dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron-forge start\"",
    "package": "electron-forge package --platform=win32 --arch=x64",
    "make": "electron-forge make --platform=win32 --arch=x64",
    "publish": "electron-forge publish --platform=win32 --arch=x64",
    "clean": "rm -rf out/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/publisher-github": "^7.8.0",
    "@electron/fuses": "^1.8.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^38.2.2",
    "wait-on": "^9.0.1"
  }
}
